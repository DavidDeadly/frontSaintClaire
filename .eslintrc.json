{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-plusplus": "off",
    "import/extensions": "off",
    "no-return-assign": "off",
    "no-unused-expressions": "off",
    "import/no-unresolved": "off",
    "one-var": "off",
    "no-alert": "off",
    "no-console": "off"
  }
}